{
  "name": "@binocolo/cli",
  "version": "0.3.0",
  "files": [
    "src/**/*",
    "dist/**/*",
    "frontend-build/**/*"
  ],
  "type": "module",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "clean:dist": "rimraf dist",
    "compile": "tsc",
    "check-compilation": "tsc --noEmit",
    "cli": "node dist/main.js",
    "server-dev": "node dist/main.js -v -o 127.0.0.1 -p 63546 -n",
    "test": "npm run compile && mocha -r ts-node/register 'dist/tests/**/*.js'"
  },
  "bin": {
    "binocolo": "bin/cli.js"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.228.0",
    "@binocolo/aws": "*",
    "@binocolo/backend": "*",
    "@binocolo/common": "*",
    "@binocolo/serialization": "*",
    "@fastify/static": "^6.6.0",
    "@fastify/type-provider-typebox": "^2.3.0",
    "@fastify/websocket": "^7.1.1",
    "@sinclair/typebox": "^0.25.20",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "conf": "^10.2.0",
    "cross-spawn": "^7.0.3",
    "date-fns": "^2.29.3",
    "fastify": "^4.7.0",
    "http-errors": "^2.0.0",
    "inquirer": "^9.1.4",
    "open": "^8.4.0",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/cross-spawn": "^6.0.2",
    "@types/http-errors": "^1.8.2",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.8.0",
    "@types/pg": "^8.6.5",
    "@types/sinonjs__fake-timers": "^8.1.2",
    "chai": "^4.3.6"
  }
}
